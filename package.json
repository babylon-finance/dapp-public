  "scripts": {
    "run-local": "echo '{}' > lambda/shared/version.json && ntl dev",
    "start": "node scripts/start.js",
    "update-autotasks": "(cd autotasks/keeper/ && yarn build) && (cd autotasks/snapper/ && yarn build) && (cd autotasks/accountant && yarn build) && (cd autotasks/whip && yarn build) && node scripts/upload-autotasks.js",
    "update-keeper": "cd autotasks/keeper/ && yarn build && cd ../../ && node scripts/upload-autotasks.js keeper",
    "update-accountant": "cd autotasks/accountant/ && yarn build && cd ../../ && node scripts/upload-autotasks.js accountant",
    "update-heart": "cd autotasks/heart/ && yarn build && cd ../../ && node scripts/upload-autotasks.js heart",
    "update-banker": "cd autotasks/banker/ && yarn build && cd ../../ && node scripts/upload-autotasks.js banker",
    "update-snapper": "cd autotasks/snapper/ && yarn build && cd ../../ && node scripts/upload-autotasks.js snapper",
    "update-whip": "cd autotasks/whip/ && yarn build && cd ../../ && node scripts/upload-autotasks.js whip",
    "run-keeper": "cd autotasks/keeper && yarn run-local",
    "build": "CI='' echo '{}' > src/31337.json && echo '{}' > lambda/shared/31337.json && node --max_old_space_size=4096 scripts/build.js && yarn run prepare-lambdas",
    "copy-tokens": "rm -f src/babylon-token-list && rm -f autotasks/common/babylon-token-list.json && cp ../protocol/babylon-token-list.json src/babylon-token-list.json && cp ../protocol/babylon-token-list.json autotasks/common/babylon-token-list.json",
    "copy-contracts-mainnet": "rm -f src/1.json && rm -rf lambda/abi && rm -rf src/constants/addresses.js && cp ../protocol/contracts.json ./src/1.json && cp ../protocol/contracts.json ./lambda/shared/1.json && cp ../protocol/lib/addresses.js src/constants/addresses.js",
    "copy-contracts-hardhat": "rm -f src/31337.json && rm -rf lambda/abi && cp ../protocol/contracts.json ./src/31337.json && cp ../protocol/contracts.json ./lambda/shared/31337.json",
    "test": "node scripts/test.js && (cd autotasks/keeper/ && yarn test) && (cd autotasks/snapper/ && yarn test) && (cd autotasks/accountant/ && yarn test)",
    "setup-schema": "node scripts/setup-schema.js",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prepare": "husky install",
    "prepare-lambdas": "./scripts/lambda-build.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/graphql": "^14.5.0",
    "@types/mixpanel-browser": "^2.35.7",
    "@types/numeral": "^2.0.1",
    "@types/react-router": "^5.1.13",
    "@types/react-select": "^4.0.14",
    "@types/react-toggle": "^4.0.2",
    "apollo": "^2.32.5",
    "defender-autotask-client": "^1.7.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faunadb": "^4.1.1",
    "git-format-staged": "^2.1.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "netlify-lambda": "^2.0.5",
    "prettier": "2.3.0",
    "pretty-quick": "^3.1.0",
    "readline": "^1.3.0",
    "surge": "^0.21.6",
    "thread-loader": "^3.0.4",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleRoots": [
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
